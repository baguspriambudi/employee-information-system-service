<form action="http://localhost:2000/api/v1/auth/employee/search" method="POST">
    <input type="text" name="search" placeholder="ketikan sesuatu">
    <button>Send</button>
</form>

<h2>Data Karyawan</h2>
<table border="1">
    <thead>
        <th>Nip</th>
        <th>Name</th>
        <th>Gender</th>
        <th>Birthdate</th>
        <th>Entrydate</th>
        <th>Grade</th>
        <th>Salary</th>
    </thead>
    <tbody>
        {{#searchEmployee}}
        <tr>
            <td>{{nip}}</td>
            <td>{{name}}</td>
            <td>{{gender}}</td>
            <td>{{birthdate}}</td>
            <td>{{entrydate}}</td>
            <td>{{grade.grade}}</td>
            <td>{{grade.salary}}</td>
        </tr>
        {{/searchEmployee}}
    </tbody>
</table></br>

<form action="http://localhost:2000/input" method="GET">
    <button>Back</button>
</form>